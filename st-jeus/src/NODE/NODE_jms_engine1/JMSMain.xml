<?xml version="1.0"?>
<jms-server xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <service-config>
	    <name>jmstest</name>
	    <port>19741</port>  
	    <blocking-socket>true</blocking-socket>
	    <use-ssl>false</use-ssl>
	    <connection-timeout>1000</connection-timeout>
	    <client-keepalive-timeout>10</client-keepalive-timeout>
	</service-config>    
	<thread-pool>
		<min>10</min>
		<max>20</max>
	</thread-pool>
    <connection-factory>
        <type>queue</type>
        <name>QueueConnectionFactory</name>
    </connection-factory>
    <connection-factory>
        <type>topic</type>
        <name>TopicConnectionFactory</name>
    </connection-factory>
    <destination>
        <type>queue</type>
        <name>NC_ASYNC_REQ_MDB_QUEUE</name>
        <export-name>jms/nexcore/async/REQ_MDB_QUEUE</export-name>
    </destination>
    <destination>
        <type>topic</type>
        <name>NC_ASYNC_REQ_MDB_TOPIC</name>
        <export-name>jms/nexcore/async/REQ_MDB_TOPIC</export-name>
    </destination>
    <destination>
        <type>topic</type>
        <name>NC_BATCH_JMS-NOTIFIER_TOPIC</name>
        <export-name>jms/topic/nexcore/batch/DefaultJmsNotifier</export-name>
    </destination>
    <!--
    <destination>
        <type>queue</type>
        <name>NC_FAULT_ALERT_QUEUE</name>
        <export-name>jms/queue/nexcore/monitoring/FAULT_ALERT_QUEUE</export-name>
    </destination>
    <destination>
        <type>queue</type>
        <name>NC_PMONITOR_QUEUE</name>
        <export-name>jms/queue/nexcore/monitoring/PMONITOR_QUEUE</export-name>
    </destination>
    -->
</jms-server>

//==============================================================================
// 공통 스크립트 로딩
//==============================================================================
#include "lib::lib_commF.js"
#include "lib::lib_commMsg.js"

//==============================================================================
// 폼 변수 정의
//==============================================================================
var FV_CURR_ROW;
var nCnt = 0;  
var nEndCnt = 0;

/*******************************************************************************
 * @description    : 최초 폼 로드시 발생 - 이벤트
*******************************************************************************/
function DISMIG00100_OnLoadCompleted(obj)
{
	div_Search.edt_filePath.Text = "";
	mskedt_TotalCnt.Value = 0;
	mskedt_EndCnt.Value = 0;
	
	// 그리드 체크박스 설정 및 정렬
	cf_setGrdChecknSortEvent(grd_upLoadXlsList);
}

/*******************************************************************************
 * @description    : 콜백
 * @param sSvcID   : 서비스 아이디
*******************************************************************************/
function f_callBack(sSvcID)
{	
	if(sSvcID == "svcDisProdInfo")
	{
		cf_copyDataSet(ds_upLoadXlsList, ds_upLoadXlsCheck,,true,true);

		nCnt = 0;
		ds_upLoadXlsCheck.ClearData();
	}
	
	if(sSvcID == "svcSaveDisProdInfo")
	{
		nCnt = 0;
		ds_upLoadXlsSave.ClearData();
	}

	if(sSvcID == "svcUpdateDisInfo")
	{
		nCnt = 0;
		ds_upLoadXlsSave.ClearData();
	}
}

/*******************************************************************************
 공통 스크립트 영역 끝
*******************************************************************************/

/*******************************************************************************
 * @description    : 엑셀업로드 - 이벤트
*******************************************************************************/
function btn_search_OnClick(obj)
{
    //데이터셋 초기화 및 컬럼 재정의
	ds_upLoadXlsList.ClearData();

	var	sDsName = "ds_upLoadXlsList"; //데이터셋
	var	nUseDsInfo = 1;	              //기본정보
	var	nStartIndex = 2;              //시작index

	mskedt_TotalCnt.Value = 0;
	mskedt_EndCnt.Value = 0;

	div_Search.fld_excel.Type = "OPEN";
	var bResult = div_Search.fld_excel.Open();
	
	if ( bResult )
	{
		var sFullPath = div_Search.fld_excel.FilePath + '\\' + div_Search.fld_excel.FileName;
		div_Search.edt_filePath.Text = sFullPath;

		return ext_ExcelImportByIndex(sFullPath, 0, sDsName, nUseDsInfo, 0, 0, nStartIndex);
	}
			
}

/*******************************************************************************
 * @description    : 재고상품정보조회  OnClick 이벤트
*******************************************************************************/
function div_Search_btn_DisProdInfo_OnClick(obj)
{
	nCnt = 0;
	
	ds_upLoadXlsCopy.ClearData();

	cf_copyDataSet(ds_upLoadXlsCopy, ds_upLoadXlsList);
		
	ds_upLoadXlsList.ClearData();

	for (var i = 0; i < ds_upLoadXlsCopy.GetRowCount(); i++)
	{
		++nCnt;

		cf_copyDataSet(ds_upLoadXlsCheck, ds_upLoadXlsCopy, i, true, true);
		
		if (nCnt == 500)
		{
			// 트랜잭션 설정
			var sSvcID = "svcDisProdInfo";
			var sInDs = "ds_upLoadXlsCheck=ds_upLoadXlsCheck";
			var sOutDs = "ds_upLoadXlsCheck=ds_upLoadXlsCheck";
			var sArg = "sktst.dis.MIG#getDisProdInfo";
			
			// 공통 트랜잭션 호출
			cf_Transaction(sSvcID, sInDs, sOutDs, sArg, null, true, true);		
		}
	}
	
	if (nCnt > 0)
	{
		// 트랜잭션 설정
		var sSvcID = "svcDisProdInfo";
		var sInDs = "ds_upLoadXlsCheck=ds_upLoadXlsCheck";
		var sOutDs = "ds_upLoadXlsCheck=ds_upLoadXlsCheck";
		var sArg = "sktst.dis.MIG#getDisProdInfo";
		
		// 공통 트랜잭션 호출
		cf_Transaction(sSvcID, sInDs, sOutDs, sArg, null, true, true);		
	}
}

/*******************************************************************************
 * @description    : 재고상품정보저장  OnClick 이벤트
*******************************************************************************/
function btn_update_OnClick(obj)
{
	nCnt = 0;
	nEndCnt = 0;

	ds_upLoadXlsSave.ClearData();
	mskedt_TotalCnt.Value = ds_upLoadXlsList.GetRowCount();

	for (var i = 0; i < ds_upLoadXlsList.GetRowCount(); i++)
	{
		++nCnt;
		++nEndCnt;
		
		mskedt_EndCnt.Value = nEndCnt;
		
		cf_copyDataSet(ds_upLoadXlsSave, ds_upLoadXlsList, i, true, true);
		
		if (nCnt == 500)
		{
			//트랜잭션 설정
			var sSvcID = "svcSaveDisProdInfo";
			var sInDs = "ds_upLoadXlsSave=ds_upLoadXlsSave:A";
			var sOutDs = "";
			var sArg = "sktst.dis.MIG#updateDisInfo";
			
			//공통 트랜잭션 호출
			cf_Transaction(sSvcID, sInDs, sOutDs, sArg, null, true, true);			
		}
	}
	
	if (nCnt > 0)
	{
		//트랜잭션 설정

/*******************************************************************************
 * @description    : 최초 폼 로드시 발생 - 이벤트
*******************************************************************************/
function ACCSPL00300_OnLoadCompleted(obj)
{
	cf_setGrdChecknSortEvent(grd_list); 
	
	cf_setAuth(pFrmNum);
	
	// 공통 코드를 위한 데이타셋 설정
	cf_setCommonCodeDsEnv();
	cf_setCommonCode("div_search.cmb_saleNet", "ZPOL_C_00105", "전체"); // 영업망 : ZPOL_C_00100
	cf_setCommonCode("div_search.cmb_saleCl", "ZSAL_C_00260"); // 영업구분 : ZPOL_C_00100
	cf_setCommonCode("div_search.cmb_dsNet", "ZPOL_C_00110",  "전체"); //유통망 : ZPOL_C_00110
	cf_setCommonCode("div_search.cmb_saleTyp", 	"ZPOL_C_00020", "전체"); //판매유형	
	http.Sync = true;
	// 설정된 코드를 기반으로 공통 코드를 추가 및 데이타 바인딩
	cf_checkCommonCode();
	
	f_init();	//폼 초기화
	uf_enableAgencyByAuth("div_search", "edt_agencyNm,edt_agencyId,btn_agency", "edt_agency");
	uf_enableDealByAuth("div_search", "edt_dealNm,edt_dealCd,btn_dealCo");
	
	//담당자 
	if (uf_getAuthCode() == 2) {
		div_search.cmb_chrgrUser.Index = 0;
		div_search.cmb_chrgrUser.Enable = false;
	}

	http.Sync = false;
}

/*******************************************************************************
 * @description    : 폼 초기화 
*******************************************************************************/
function f_init()
{
	div_search.cal_from.Value = substr(today(),0,6) + "01";
	div_search.cal_to.Value = today();
	ds_rpl_sale_cond.AddRow();
	ds_condition.AddRow();
	ds_cond_chrgr_id.AddRow();
	div_search.cmb_saleTyp.Index = 0;
	div_search.cmb_saleCl.Index = 4;
}


/*******************************************************************************
 * @description    :  콜백
*******************************************************************************/
function f_callBack(sSvcID)
{	
	if (sSvcID == "svcList1") {
	
		http.Sync = true;
		ds_get_chrgr_id.InsertRow(0);
		ds_get_chrgr_id.SetColumn(0, "user_nm", "전체");
		div_search.cmb_chrgrUser.Index = 0;
		http.Sync = false;
		
	}
	else if(sSvcID == "svcList"){
		ds_rpl_sale_cond_tmp.Clear();
		ds_rpl_sale_cond_tmp.Copy(ds_rpl_sale_cond);
	}
	else if (sSvcID =="getDtlEx"){
		cf_exportExcel(grd_dtlEx);
	}

}


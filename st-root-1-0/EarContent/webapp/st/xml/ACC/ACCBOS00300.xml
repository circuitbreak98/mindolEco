function f_search(){
	
	btn_search_OnClick();

}


/*******************************************************************************
 * @description    : 리스트 조회 - 이벤트
*******************************************************************************/
function btn_search_OnClick(obj)
{
	if(!cf_checkObjValues(div_search)){
		return;
	}
	
	if ( uf_IsNull(div_search.cal_acc_mth.Value) )
    {
        alert(cf_getMessage(MSG_00028, "정산일"));
        div_search.cal_acc_mth.SetFocus();
        return false;
    }
	
	if(uf_IsNull(div_search.edt_agencyCd.value)){

		alert(cf_getMessage(MSG_00028, "대리점"));
		div_search.edt_agencyNm.SetFocus();
		return;
	}
	
	
	if(uf_isNull(div_search.cal_acc_mth.Value) == false){ 
		var sLength =div_search.cal_acc_mth.Value.length();
		var targetDay = "20091101";
		
		if(sLength ==6){
			if(substr(targetDay,0,6) > div_search.cal_acc_mth.Value) {
				ds_condition.SetColumn(0,"ACC_DT", div_search.cal_acc_mth.Value);
				div_search.cal_acc_mth.SetFocus();
	
				alert("["+substr(targetDay,0,4)+"-"+substr(targetDay,4,2)+"] 이후로는 선택할 수 없습니다.");
				return false;
			}
		}else{
			if(targetDay > div_search.cal_acc_mth.Value) {
				ds_condition.SetColumn(0,"ACC_DT", div_search.cal_acc_mth.Value);
				div_search.cal_acc_mth.SetFocus();
	
				alert("["+substr(targetDay,0,4)+"-"+substr(targetDay,4,2)+"-"+substr(targetDay,6,2)+"] 이후로는 선택할 수 없습니다.");
				return false;
			}
		}
	
	}
	
	cf_setParam(ds_condition,"ACC_DT_PRE",AddDate(ds_condition.GetColumn(0,"ACC_DT"),-1));
	
	// 트랜잭션 설정
	var sSvcID = "svcList";
	var sInDs  = "nc_input_fieldDs=ds_condition";
	var sOutDs = "ds_dpst_list=ds_dpst_list";
	var sArg   = "sktst.acc.BOS#getDpstAccStatus";
	
	// 공통 트랜잭션 호출
	cf_Transaction(sSvcID, sInDs, sOutDs, sArg,null,true,true);
}



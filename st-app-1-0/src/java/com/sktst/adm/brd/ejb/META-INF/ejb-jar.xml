<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
<!-- ejb id : BRD -->

 <enterprise-beans>
     <session id="Session_sktst.adm.BRD">
         <display-name>sktst.adm.BRD</display-name>
         <ejb-name>sktst.adm.BRD</ejb-name>
         <home>com.sktst.adm.brd.ejb.BRDHome</home>
         <remote>com.sktst.adm.brd.ejb.BRD</remote>
         <ejb-class>com.sktst.adm.brd.ejb.BRDBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
     </session>
 </enterprise-beans>

 <assembly-descriptor>
     <container-transaction>
         <method>
             <ejb-name>sktst.adm.BRD</ejb-name>
             <method-name>getBoardList</method-name>
         </method>
         <trans-attribute>Required</trans-attribute>
     </container-transaction>
     <container-transaction>
         <method>
             <ejb-name>sktst.adm.BRD</ejb-name>
             <method-name>deleteBoard</method-name>
         </method>
         <trans-attribute>Required</trans-attribute>
     </container-transaction>
     <container-transaction>
         <method>
             <ejb-name>sktst.adm.BRD</ejb-name>
             <method-name>getBoard</method-name>
         </method>
         <trans-attribute>Required</trans-attribute>
     </container-transaction>
     <container-transaction>
         <method>
             <ejb-name>sktst.adm.BRD</ejb-name>
             <method-name>saveBoard</method-name>
         </method>
         <trans-attribute>Required</trans-attribute>
     </container-transaction>
     <container-transaction>
         <method>
             <ejb-name>sktst.adm.BRD</ejb-name>
             <method-name>addAttachFiles</method-name>
         </method>
         <trans-attribute>Required</trans-attribute>
     </container-transaction>

 </assembly-descriptor>
</ejb-jar>
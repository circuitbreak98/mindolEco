    ,COLOR_CD        
    ,SVC_DT          
    ,EQP_ST          
    ,USEPRD_DDCT_AMT 
    ,CORROSION_YN    
    ,POWER_YN        
    ,CALL_YN         
    ,DATA_YN         
    ,DISP_YN          
    ,FUNCTION_YN     
    ,LOST_YN         
    ,RETURN_YN       
    ,LEASE_YN        
    ,FLOOD_YN        
    ,USIM_YN         
    ,OTHER_YN        
    ,RMKS            
    ,CONFIRM_YN      
    ,FULLSET_YN      
    ,PRCHS_AMT
    ,BUY_CL  
    ,RES_NO

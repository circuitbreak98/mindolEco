/*******************************************************************************
 * @description    : 최초 폼 로드시 발생 - 이벤트
*******************************************************************************/
function SALSMG03000_OnLoadCompleted(obj)
{
	cf_setAuth(pFrmNum);	   // 권한 공통
}


/*******************************************************************************
 * @description    : 콜백
 * @param sSvcID   : 서비스 아이디
*******************************************************************************/
function f_callBack(sSvcID)
{
    if (sSvcID == "getGeneralSale")
    {
        if (ds_GeneralSale.rowcount = 0) 
        {
			f_Init_Dataset();

			alert("등록된 판매정보가 없습니다.");
			div_dealCoInfo.edt_svcMgmtNum.SetFocus() ;
			return;			
        }

        if (ds_PolMaster.colcount > 0) 
        {
			f_PolDetailSearch();
			return;			
        } 
    }
}


/*******************************************************************************
 * @description    : 서비스관리번호 Enter Key Down --> 조회
*******************************************************************************/
function div_dealCoInfo_edt_svcMgmtNum_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13)
	{
		f_Search();
	}
}


/*******************************************************************************
 * @description    : 개통정보 조회
 * @param sSvcID   : 서비스관리번호 기준
*******************************************************************************/
function f_Search()
{
	ds_input.Clear();

    if ( length(div_dealCoInfo.edt_svcMgmtNum.Text) <> 10 )
    {
        alert("서비스관리번호를 확인하세요.");
        div_dealCoInfo.edt_svcMgmtNum.SetFocus() ;
        return;
    }

	FV_SvcMgmtNum = div_dealCoInfo.edt_svcMgmtNum.Text;
    cf_setParam(ds_input, "SVC_MGMT_NUM", div_dealCoInfo.edt_svcMgmtNum.Text );		// 서비스관리번호

 	var sSvcID = "getGeneralSale";
	var sInDs  = "nc_input_fieldDs=ds_input";
	var sOutDs = "ds_GeneralSale=ds_GeneralSale ds_Suplment=ds_Suplment ds_PolMaster=ds_PolMaster";
	var sArg   = "sktst.sal.SMG#getGeneralSale";	

	cf_Transaction(sSvcID, sInDs, sOutDs, sArg, "", true, true);
}


/*******************************************************************************
 * @description    : 부가상품 장려금 조회
 * @param sSvcID   : 정책 기준
*******************************************************************************/
function f_PolDetailSearch()
{
	ds_input.Clear();

    cf_setParam(ds_input, "IPOL_ID", ds_PolMaster.GetColumn(ds_PolMaster.currow, "IPOL_ID") );	// 정책 ID
    cf_setParam(ds_input, "POL_YM",  ds_PolMaster.GetColumn(ds_PolMaster.currow, "POL_YM") );	// 정책 년월
    cf_setParam(ds_input, "POL_TS",  ds_PolMaster.GetColumn(ds_PolMaster.currow, "POL_TS") );	// 정책 차수

 	var sSvcID = "getPolDetail";
	var sInDs  = "nc_input_fieldDs=ds_input";
	var sOutDs = "ds_PolDetail=ds_PolDetail";
	var sArg   = "sktst.sal.SMG#getPolPayInfo";	

	cf_Transaction(sSvcID, sInDs, sOutDs, sArg, "", true, true);
}



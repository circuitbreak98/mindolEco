*******************************************************************************/
function btn_save_OnClick(obj)
{
	FV_SAVE_CHECK =1;
	
	if(toNumber(ds_crdtPop.GetColumn(0,"PCT")) > 2000){
		alert("순담보비율은 2,000% 이하로 입력해주십시오.");
		div_input.edt_crdtPct.SetFocus();
		return false;			 
	 }
	 
	 
	 if(toNumber(ds_crdtPop.GetColumn(0,"PCT")) == 0){
		alert("순담보비율은 0% 이상으로 입력해주십시오.");
		div_input.edt_crdtPct.SetFocus();
		return false;			 
	 }
	 
	 
	 if(Length(div_input.txa_CnclRsn.Value) < 1){
		alert("비고 는 필수입력 항목입니다.");
		div_input.txa_CnclRsn.SetFocus();
		return false;		 
	 }
	 

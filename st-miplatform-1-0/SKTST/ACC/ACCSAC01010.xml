	sArg += " SALE_GB=" + quote(ds_monthClsDetail.GetColumn(nRow,"SALE_GB")); 
	sArg += " SUPL_PROD_SALE_NO=" + quote(ds_monthClsDetail.GetColumn(nRow,"SUPL_PROD_SALE_NO")); 

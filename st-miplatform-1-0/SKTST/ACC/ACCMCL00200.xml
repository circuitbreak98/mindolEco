*******************************************************************************/

//==============================================================================
// 공통 스크립트 로딩
//==============================================================================
#include "lib::lib_commF.js"
#include "lib::lib_commMsg.js"
#include "lib::lib_util.js"
#include "lib::lib_commPopup.js"

/*******************************************************************************
// 2. 폼 변수 정의
*******************************************************************************/
var FV_POS_AGENCY_ID;		// 대리점 코드
var FV_POS_AGENCY_UKEY_ID;	// 대리점 UKey 코드
var FV_POS_AGENCY_NM;		// 대리점명
var FV_CLOSE_DT;

/*******************************************************************************
* 함수명 : ACCMCL00200_OnLoadCompleted
* 기능명 : OnLoadCompleted 이벤트
*******************************************************************************/
function ACCMCL00200_OnLoadCompleted(obj)
{
	// 권한 설정
	cf_setAuth(pFrmNum);
	
	http.Sync = true;
	
	FV_POS_AGENCY_ID      = gds_session.GetColumn(0,"posAgencyId");   
    FV_POS_AGENCY_NM      = gds_session.GetColumn(0,"posAgencyNm");     
    FV_POS_AGENCY_UKEY_ID = gds_session.GetColumn(0,"posAgencyUkeyId");
    
    if( !uf_IsNull(FV_POS_AGENCY_ID) ){
		cf_setParam(ds_condition,"POS_AGENCY", FV_POS_AGENCY_ID);
    }
    
	f_getCloseBisMth();
	
	http.Sync = false;

}

/*******************************************************************************
* 함수명 : f_callBack
* 기능명 : 트랜잭션 콜백
* 파라미터
1. sSvcID  : 서비스 아이디
*******************************************************************************/
function f_callBack(sSvcID)
{
	if(sSvcID == "svcSearch")
	{
		return;
	}
	
	if(sSvcID == "svcSearchGetCloseBisMth")
	{
		if(ds_closeBisMth.rowcount == 0)
		{
			alert("수행된 월마감이 없습니다");
			div_search.btn_search.Enable = false;
		}
		else
		{
			cf_setParam(ds_condition,"CLS_MTH", ds_closeBisMth.GetColumn(0,"CLS_MTH"));
			div_search.btn_search.Enable = true;
		}
		
		return;
		
	}
	
	if(sSvcID == "svcSearchGetBisMth")
	{
		return;
		
	}
	
}

/*******************************************************************************
* 함수명 : df_search
* 기능명 : 공통 조회 버튼 클릭
* 파라미터
1. sButtonID  : 버튼 아이디
*******************************************************************************/
function df_search()
{	

	if(uf_isNull(ds_condition.GetColumn(0,"CLS_MTH")) == true){
		alert(cf_getMessage(MSG_00135, "정산월"));
		div_search.cal_accYm.SetFocus();
		return false; 
	}
	
	ds_monthClsList.GroupKey = "ORG_ID_2";
	
	var sSvcID = "svcSearch";
	var sInDs  = "nc_input_fieldDs=ds_condition";
	var sOutDs = "ds_monthClsList=ds_monthClsList";
	var sArg   = "sktst.acc.MCL#getMthClsStatusList";
	
	cf_Transaction(sSvcID, sInDs, sOutDs, sArg, null, true, true);
}

/*******************************************************************************
* 함수명 : f_getCloseBisMth
* 기능명 : 최종마감월 조회
*******************************************************************************/
function f_getCloseBisMth()
{
	var sSvcID = "svcSearchGetCloseBisMth";
	var sInDs  = "nc_input_fieldDs=ds_condition";
	var sOutDs = "ds_closeBisMth=ds_closeBisMth";
	var sArg   = "sktst.acc.MCL#getCloseBisMth";
	
	cf_Transaction(sSvcID, sInDs, sOutDs, sArg);
}

/*******************************************************************************
* 함수명 : f_getBisMth
* 기능명 : 수행가능한 월마감 조회
*******************************************************************************/
function f_getBisMth()
{
	var sSvcID = "svcSearchGetBisMth";
	var sInDs  = "nc_input_fieldDs=ds_condition";
	var sOutDs = "ds_bisMth=ds_bisMth";
	var sArg   = "sktst.acc.MCL#getBisMth";
	
	cf_Transaction(sSvcID, sInDs, sOutDs, sArg);
}

/*******************************************************************************
 * @description    :  월달력 사용 - 이벤트
*******************************************************************************/
function btn_monthCalendar_OnClick(obj)
{
	cf_callMonthCalendar(div_search.cal_accYm);
	
	http.Sync = true;
	
	f_getBisMth(); 
	
	if(ds_bisMth.rowcount == 0){
		alert("월마감이 수행되지 않았습니다.");
		cf_setParam(ds_condition,"CLS_MTH", ds_closeBisMth.GetColumn(0,"CLS_MTH"));
	}
	
	http.Sync = false;

}

/*******************************************************************************
 * Desc   : 월마감현황 리스트 클릭시 이벤트 발생
 * Auth   : 전승호 (2009.01.15) 
*******************************************************************************/
function grd_monthClsList_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	var oGrdDs = object(obj.BindDataset);
	
	if( !uf_IsNull(oGrdDs.getColumn(nRow,"ORG_ID_4"))){
	
		var sArg  = "ORG_ID=" + quote(oGrdDs.getColumn(nRow,"ORG_ID_4")); 
			sArg += " CLS_MTH=" + quote(oGrdDs.getColumn(nRow,"CLS_MTH"));
			sArg += " DEAL_CO_NM=" + quote(oGrdDs.getColumn(nRow,"ORG_NM_4"));
			
		dialog("ACC::ACCMCL00210.xml", sArg, 968, 440);
	}
}

/*******************************************************************************
* 함수명 : btn_export_OnClick
* 기능명 :  엑셀 export
*******************************************************************************/
function btn_export_OnClick(obj)
{
	cf_exportExcel(grd_monthClsList);
}


